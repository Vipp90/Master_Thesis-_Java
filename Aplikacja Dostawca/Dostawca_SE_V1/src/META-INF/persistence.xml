<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="Dostawca_SEPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Warstwa_biznesowa.Klient</class>
    <class>Warstwa_biznesowa.Pozycja_zamowienia</class>
    <class>Warstwa_biznesowa.Produkt</class>
    <class>Warstwa_biznesowa.Zamowienie</class>
    <class>Warstwa_biznesowa.Zapytania</class>
    <class>Warstwa_Biznesowa1.Klient1</class>
    <class>Warstwa_Biznesowa1.Produkt1</class>
    <class>Warstwa_Biznesowa1.Zamowienie1</class>
    <class>Warstwa_Biznesowa1.Zapytania1</class>
    <class>Warstwa_Biznesowa1.Encja</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/Dostawca"/>
      <property name="javax.persistence.jdbc.password" value="Dostawca"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.user" value="Dostawca"/>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
